            <string>{
    //Moving Left and Right
    if (keyboard_check(vk_right) &amp;&amp; place_free(x+4,y) &amp;&amp; !keyboard_check(vk_left))  {
        if (place_free (x,y+1)) {
            if (place_free(x+4,y=vspeed)) {
                sprite_index =  spr_player_run;
                image_speed = .4;
                image_xscale = 1;
                x+=4;
            }
        } else {
                sprite_index =  spr_player_run;
                image_speed = .4;
                image_xscale = 1;
            x+=4;
        }
    } else if (keyboard_check(vk_left)  &amp;&amp; place_free(x-4,y) &amp;&amp; !keyboard_check(vk_right)) {
        if (place_free (x,y+1)) {
            if (place_free(x-4,y+vspeed)) {
                sprite_index =  spr_player_run;
                image_speed = .4;
                image_xscale = -1;
                x-=4;
            }
        } else {
            x-=4;
                sprite_index =  spr_player_run;
                image_speed = .4;
                image_xscale = -1;
        }
    } else{
        sprite_index = spr_player_breath;
        image_speed = .1
        }
    
    //Gravity
    if (place_free(x,y+1)) {
        gravity = .75;
        sprite_index = spr_player_jump;
        
        if (vspeed &gt;= 0) {
            image_index = 0;
        } else {
            image_index = 1;
        }
        
        //Set max fall speed
        if(vspeed &gt;= 10) {
           vspeed = 10;
        }
    } else  {
        gravity =  0;
        
        //Jumping
        if (keyboard_check_pressed(vk_up)) {
            vspeed = -10
        }
    }
}
</string>
